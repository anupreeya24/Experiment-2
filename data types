#include<iostream>
using namespace std;

int main(){
    int myintvar;
    float myfloatvar;
    double mydoublevar;
    char mycharvar;
    string mystringvar;
    bool myboolvar;

    cout <<"enter integer";
    cin>>myintvar;

    cout <<"enter float";
    cin>>myfloatvar;

    cout <<"enter double";
    cin>>mydoublevar;

    cout <<"enter character";
    cin>>mycharvar;

    cout<<"enter string "; 
    cin>>mystringvar;

    cout<<"enter bool" ;
    cin>>myboolvar;

}
